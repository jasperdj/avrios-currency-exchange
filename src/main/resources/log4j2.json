{
  "configuration": {
    "name": "log-enrichment",
    "appenders": {
      "Socket": {
        "name": "socket",
        "host": "localhost",
        "port": "9500",
        "JSONLayout": {
          "complete": false,
          "compact": true,
          "eventEol": false
        }
      },
      "Console": {
        "name": "console",
        "LevelRangeFilter": {
          "minLevel": "fatal",
          "maxLevel": "info",
          "onMatch": "ACCEPT",
          "onMismatch": "DENY"
        },
        "target": "SYSTEM_OUT",
        "PatternLayout": {
          "pattern": "%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"
        }
      }
    },
    "loggers": {
      "AsyncLogger": [
        {
          "name": "com.avrios.sample.exchange",
          "level": "trace",
          "additivity": "false",
          "appender-ref": [
            {
              "ref": "socket"
            },
            {
              "ref": "console"
            }
          ]
        }
      ],
      "root": {
        "level": "info",
        "appender-ref": [
          {
            "ref": "console"
          }
        ]
      }
    }
  }
}